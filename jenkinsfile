pipeline {
    agent { label 'exercise'}
    stages {
        stage ('vcs') {
            steps {
                git url: 'https://github.com/Potulajagadeesh/spring-petclinic.git',
                    branch: 'develop'
            }  
        }
        stage ('MVN PACKAGE') {
            steps {
                sh './mvnw package'
            }
        }
    }
}    